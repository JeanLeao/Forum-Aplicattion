<%- include ('../partials/header.ejs'); %>
<body>
    <%- include ('../partials/navbar.ejs'); %>
    <div class="container">
        <hr>
        <h2>Usuarios</h2>
        <a href="/admin/users/create" class="btn btn-success">Criar Usuario</a>
        <hr>

        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>EMAIL</th>
                </tr>
            </thead>
            <tbody>
            <% usuarios.forEach((user) => { %>
                <tr>
                    <td><%=user.id%></td>
                    <td><%=user.email%></td>
                </tr>
                    <% }) %>
                </tbody>
        </table>
    </div>
</body>
<%- include ('../partials/footer.ejs'); %>